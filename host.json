{
  "version": "2.0",
  "functionTimeout": "00:30:00",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "http": {
      "maxOutstandingRequests": 200,
      "maxConcurrentRequests": 100,
      "routePrefix": "api"
    }
  },
  "cors": {
    "allowedOrigins": [
      "*"
    ],
    "supportCredentials": false
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Trace"
    }
  }
}